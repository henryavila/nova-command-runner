{
    "name": "henryavila/nova-command-runner",
    "description": "Laravel Nova tool for running Artisan and bash(shell) commands.",
    "keywords": [
        "laravel",
        "nova",
        "Artisan",
        "commands",
        "shell",
        "bash"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "laravel/nova": "^4.0",
        "henryavila/laravel-helpers": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../nova"
        },
        {
            "type": "github",
            "url": "https://github.com/henryavila/laravel-helpers.git"
       }
    ],
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaCommandRunner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaCommandRunner\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
